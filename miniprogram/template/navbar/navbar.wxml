<!--template/navbar/navbar.wxml-->
<view style="height: {{statusHeight}}px;position: fixed;background-color: white;width: 100%;z-index: 9999;">
  <view bind:tap="backAction" style="display: inline-block;">
    <van-icon size="25px" name='arrow-left' style="color: #000;width: 50px;height: 50px;"></van-icon>
  </view>
  <view style="display: inline-block;text-align: center;padding-left: 8px;padding-top: {{statusHeight-44}}px;line-height: 44px;font-size: 17px;">{{title}}</view>
  
</view>
